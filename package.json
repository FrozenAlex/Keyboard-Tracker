{
  "name": "floodz-keyboard",
  "version": "0.8.0",
  "description": "keyboard tracker for twitch.tv/floodz",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"    
  },
  "build": {
    "appId": "floodz.keybord.tracker",
    "productName": "Floodz Keyboard"
  },
  "author": "GabeAlexander",
  "license": "ISC",
  "dependencies": {
    "iohook": "^0.6.5"
  },
  "devDependencies": {
    "electron": "^8.0.1",
    "browser-sync": "^2.26.7",
    "node-abi": "^2.15.0"
  },
  "iohook": {
    "targets": [
      "node-79",
      "electron-76"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  }
}
